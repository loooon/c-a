  <select id="selectByDtoPage" resultType="com.credit.entity.dto.UserResourceDto" parameterType="com.credit.dao.query.UserQuery">
    <![CDATA[
        select u.user_id as userId, u.user_name as userName ,u.vkey as vkey,u.phone as phone,u.email as email,u.expire_date as expireDate,group_concat(upe.resource_id separator '<br/>') as
        resourceId,upe.daily_times as dailyTimes,upe.yearly_times as yearlyTimes, group_concat(sr.resource_path separator '<br/>') as resourcePath ,group_concat(sr.resource_name separator '<br/>')  as resourceName from tb_user_permission  as upe right JOIN tb_user as u on upe.user_id = u.user_id left JOIN tb_site_resource  as sr on upe.resource_id = sr.resource_id
          ]]>
    <if test="userName!=null and userName !=''">
      where u.user_name like concat(#{userName},'%')
    </if>
    group by u.user_id ORDER BY
    <if test="sort.property != null and sort.property != ''">
      u.${sort.property} ${sort.direction},
    </if>
    u.user_id DESC
    <include refid="Page.footer"/>
