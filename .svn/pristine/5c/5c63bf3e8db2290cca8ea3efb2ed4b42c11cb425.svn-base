public class SiteResourceQuery extends PageQuery
{
    private String nameOrPath;

    private Sort sort;

    public String getNameOrPath()
    {
        return nameOrPath;
    }

    public void setNameOrPath(String nameOrPath)
    {
        this.nameOrPath = nameOrPath;
    }

    public Sort getSort()
    {
        return sort;
    }

    public void setSort(Sort sort)
    {
        this.sort = sort;
    }
}
